{
    "name": "apblock/apollo",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "role": "Author",
            "name": "Apollo Team",
            "email": "apollo.prjct.authr@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ApBlock\\Apollo\\": "src/"
        }
    },
    "require": {
        "php": "^7.2",
        "ext-curl": "*",
        "ext-openssl": "*",
        "ext-date": "*",
        "ext-dom": "*",
        "ext-hash": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-reflection": "*",
        "ext-simplexml": "*",
        "ext-soap": "*",
        "ext-gd": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "doctrine/doctrine-module": "4.1.3",
        "league/container": "^4.2",
        "monolog/monolog": "^2.3",
        "guzzlehttp/psr7": "^2.1",
        "league/route": "^5.1",
        "twig/twig": "^3.3",
        "doctrine/orm": "^2.10",
        "beberlei/doctrineextensions": "^1.3",
        "firebase/php-jwt": "^5.5",
        "phpmailer/phpmailer": "^6.5",
        "gedmo/doctrine-extensions": "^3.4",
        "php-curl-class/php-curl-class": "^9.5",
        "laminas/laminas-i18n": "^2.13",
        "guzzlehttp/guzzle": "^7.4"
    },
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
